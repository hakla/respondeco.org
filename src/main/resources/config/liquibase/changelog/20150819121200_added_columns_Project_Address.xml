<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <!--
        Added the entity PropertyTag.
    -->
    <changeSet id="20150819121200" author="Clemens Puehringer">
        <addColumn tableName="T_PROJECT">
            <column name="country" type="varchar(64)"/>
            <column name="zipCode" type="varchar(10)"/>
            <column name="city" type="varchar(64)"/>
            <column name="street" type="varchar(64)"/>
            <column name="streetNumber" type="int"/>
            <column name="extra" type="varchar(64)"/>
            <column name="latitude" type="double"/>
            <column name="longitude" type="double"/>
        </addColumn>

    </changeSet>
</databaseChangeLog>
